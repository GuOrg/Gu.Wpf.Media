<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:media="clr-namespace:Gu.Wpf.Media">
    <SolidColorBrush x:Key="{x:Static media:Brushes.EnabledForegroundBrushKey}" Color="White" />
    <SolidColorBrush x:Key="{x:Static media:Brushes.DisabledForegroundBrushKey}" Color="White" Opacity="0.4" />
</ResourceDictionary>